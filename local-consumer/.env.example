# Worker URLs
CONSUMER_WORKER_URL=https://vm-consumer-worker.your-subdomain.workers.dev
API_WORKER_URL=https://vm-api-worker.your-subdomain.workers.dev

# Arrakis Server
ARRAKIS_URL=http://127.0.0.1:7000


# Tunnel Configuration (systemd paths)
TUNNEL_NAME=your-tunnel-name
TUNNEL_CONFIG_PATH=/etc/cloudflared/config.yml
BASE_DOMAIN=your-base-domain


# Default VM Images
DEFAULT_KERNEL=/opt/arrakis/vmlinux.bin
DEFAULT_ROOTFS=/opt/arrakis/rootfs.ext4
DEFAULT_INITRAMFS=/opt/arrakis/initramfs.cpio.gz


