{
  "name": "vm-api-worker-d1",
  "version": "2.0.0",
  "description": "VM API Worker with D1 Database",
  "main": "src/index.ts",
  "scripts": {
    "build": "wrangler deploy --dry-run --outdir dist",
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "tail": "wrangler tail",
    "db:migrate": "wrangler d1 migrations apply vm_management",
    "db:migrate:prod": "wrangler d1 migrations apply vm_management --remote",
    "db:reset": "wrangler d1 migrations apply vm_management --local",
    "db:shell": "wrangler d1 execute vm_management --command"
  },
  "dependencies": {
    "hono": "^4.5.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/uuid": "^10.0.0",
    "@cloudflare/workers-types": "^4.20240806.0",
    "typescript": "^5.5.4",
    "wrangler": "^3.65.1"
  }
}